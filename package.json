    "scripts": {
        "start": " webpack-dev-server --progress --env.develop",
        "start:project": "cross-env BUILD_ENV=jenkins webpack --color --progress --config webpack.publish.js  --env.NODE_ENV=production  &&  webpack-dev-server --config webpack.project.js",
        "build:project": "cross-env BUILD_ENV=jenkins webpack --color --progress --config webpack.publish.js  --env.NODE_ENV=production  && webpack --color --progress --config webpack.project.js --env.production",
        "build": "webpack --color --progress --config webpack.config.js --env.production",
        "build:style": "gulp --gulpfile build-style.js",
        "build:test": "cross-env BUILD_ENV=test webpack --color --progress --config webpack.test.js --env.NODE_ENV=development",
        "build:publish": "npm i && cross-env BUILD_ENV=publish webpack --color --progress --config webpack.publish.js  --env.NODE_ENV=production",
        "zip": "node doZip.js",
        "deploy": "npm publish && node dingtalk.js",
        "test:bcl": "npm link @syman/ark-ui-bcl && npm run build:test && npm link"
    },
