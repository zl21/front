{
    "name": "@syman/burgeon-r3",
    "version": "3.6.7-beta",
    "description": "A Project for R3 Erp.",
    "main": "r3.publish/r3.min.js",
    "files": [
        "r3.publish",
        "entry"
    ],
    "scripts": {
        "start": " cross-env BUILD_ENV=1.4 webpack-dev-server --config ./webpack/webpack.config.js --progress --env.develop",
        "start:1.3": " cross-env BUILD_ENV=1.3 webpack-dev-server --config ./webpack/webpack.config.js --progress --env.develop",
        "start:project": "cross-env BUILD_ENV=publish webpack --color --progress --config ./webpack/webpack.publish.js  --env.NODE_ENV=production  &&  cross-env Version=1.4  webpack-dev-server --config ./webpack/webpack.project.js",
        "start:project_1.3": "cross-env BUILD_ENV=publish webpack --color --progress --config ./webpack/webpack.publish.js  --env.NODE_ENV=production  &&  cross-env Version=1.3  webpack-dev-server --config ./webpack/webpack.project.js",
        "start:share": " cross-env BUILD_ENV=1.4 webpack-dev-server --progress --config ./webpack/webpack.share.js",
        "build:project": "cross-env BUILD_ENV=publish webpack --color --progress --config ./webpack/webpack.publish.js  --env.NODE_ENV=production  && webpack --color --progress --config ./webpack/webpack.project.js --env.production",
        "build:project_1.3": "cross-env BUILD_ENV=publish webpack --color --progress --config ./webpack/webpack.publish.js  --env.NODE_ENV=production  && cross-env Version=1.3 webpack --color --progress --config ./webpack/webpack.project.js --env.production",
        "build:oss": "cross-env BUILD_ENV=oss webpack --color --progress --config ./webpack/webpack.config.js --env.production && npm run server",
        "build:oss_no": "cross-env BUILD_ENV=oss_no webpack --color --progress --config ./webpack/webpack.config.js --env.production  && npm run server",
        "build:share": "cross-env BUILD_ENV=oss webpack --color --progress --config ./webpack/webpack.share.js --env.production",
        "build:style": "gulp --gulpfile ./src/__utils__/build-style.js",
        "build:test": "cross-env BUILD_ENV=publish webpack --color --progress --config ./webpack/webpack.test.js --env.NODE_ENV=development",
        "build:publish": "npm i && cross-env BUILD_ENV=publish webpack --color --progress --config ./webpack/webpack.publish.js  --env.NODE_ENV=publish",
        "zip": "node ./src/__utils__/doZip.js",
        "deploy": "npm publish && node ./src/__utils__/dingtalk.js &&  npm run git",
        "server": "node ./build/server",
        "git": "node ./src/__utils__/release.js",
        "upload": "ark oss --upload --ossPath=/@syman-burgeon-r3/ --localPath=./r3.publish"
    },
    "repository": {
        "type": "git",
        "url": "git@gitee.com:bojun_front_end/r3-front-frame.git"
    },
    "author": "Burgeon Front-End Developer",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.11.4",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
        "@babel/plugin-proposal-optional-chaining": "^7.11.0",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/polyfill": "^7.10.4",
        "@babel/preset-env": "^7.11.0",
        "babel-eslint": "^10.1.0",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "babel-loader": "^8.1.0",
        "babel-plugin-import": "^1.13.3",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "clean-webpack-plugin": "^1.0.1",
        "compressing": "^1.5.1",
        "copy-webpack-plugin": "^5.1.2",
        "cross-env": "^7.0.3",
        "css-loader": "^2.1.0",
        "esbuild-loader": "^2.18.0",
        "eslint": "^5.12.1",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-import-resolver-webpack": "^0.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx": "0.0.2",
        "eslint-plugin-vue": "^5.1.0",
        "express": "^4.17.1",
        "file-loader": "^3.0.1",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^5.0.0",
        "gulp-clean-css": "^3.9.3",
        "gulp-less": "^4.0.0",
        "gulp-rename": "^1.2.2",
        "html-webpack-plugin": "^4.5.0",
        "http-proxy-middleware": "^2.0.1",
        "less": "~3.9.0",
        "less-loader": "^4.1.0",
        "mini-css-extract-plugin": "^2.6.0",
        "style-loader": "^0.23.1",
        "svg-sprite-loader": "^6.0.11",
        "terser-webpack-plugin": "^1.3.0",
        "uglifyjs-webpack-plugin": "^1.1.2",
        "url-loader": "^2.3.0",
        "connect-history-api-fallback": "^1.6.0",
        "vue-loader": "^15.9.3",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.12",
        "webpack": "^5.1.3",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@syman/ark-ui": "^1.2.8",
        "awe-dnd": "^0.3.4",
        "axios": "^0.21.1",
        "md5": "^2.3.0",
        "v-viewer": "^1.5.1",
        "vue": "^2.6.12",
        "vue-i18n": "^8.25.1",
        "vue-router": "^3.4.3",
        "vue-slicksort": "^1.1.3",
        "vuedraggable": "^2.24.1",
        "vuex": "^3.5.1",
        "webpack-theme-color-replacer-syman": "0.0.6",
        "ztree": "^3.5.24"
    },
    "peerDependencies": {
        "@syman/ark-ui": "^1.2.8"
    }
}
